        </children>
        <opaqueInsets>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </opaqueInsets>
    </GridPane>
    <HBox prefHeight="34.0" prefWidth="655.0" spacing="20.0" AnchorPane.bottomAnchor="30.0"
          AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Button mnemonicParsing="false" onAction="#handleSendRequest" text="Send request"/>
        <Button onAction="#handleBack" mnemonicParsing="false" text="Back"/>
    </HBox>
</AnchorPane>
